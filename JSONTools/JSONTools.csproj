<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.1</Version>
    <OutputType>Library</OutputType>
    <Title>JSONTools</Title>
    <Description>CDON Marketplace</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Ongoal Sweden AB</Company>
    <Authors>Tommy Ivarsson</Authors>
    <PackageProjectUrl>https://github.com/tomeo/JSONTools</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
